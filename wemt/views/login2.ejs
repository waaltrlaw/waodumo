                                                        <script>


                                                            function closeLoginAlert(x) {
                                                                document.querySelector("[data-alertkey='" + x.attributes["data-close-target"].value + "']").remove()
                                                            }

                                                            function handleLoginAlerts() {
                                                                var today = new Date();

                                                                var showAlert = function (a, z) {
                                                                    document.querySelector(".alerts-container").classList.remove("empty");
                                                                    document.querySelector(".alerts-container").innerHTML += "<div class='alert-item' data-alertkey='" + z + "'><p>" + a.alertcopy + "</p><button class='close' onclick='closeLoginAlert(this)' data-close-target='" + z + "' aria-label='Close Alert'> <svg class='icon-close'> <use xlink:href='#icon-close'></use> </svg> </button></div>";
                                                                }

                                                                var getAlerts = function () {
                                                                    for (var i in allAlerts) {
                                                                        var alert = allAlerts[i];
                                                                        if (/true/gi.test(alert.showloginealert)) {
                                                                            var started = new Date(alert.startdate);
                                                                            var ended = new Date(alert.enddate);

                                                                            if (today >= started && today <= ended) {
                                                                                showAlert(alert, i);
                                                                            }
                                                                        }
                                                                    }
                                                                }

                                                                getAlerts();
                                                            }

                                                            $(document).ready(function () {

                                                                try {
                                                                    // Detect IE11
                                                                    if (!!window.MSInputMethodContext && !!document.documentMode) {
                                                                        $('body').addClass('legacy');
                                                                    }
                                                                }
                                                                catch (err) { }

                                                                if (typeof (Granite) !== 'undefined') {
                                                                    $('body').addClass('granite');
                                                                }
                                                                /*
                                                                   else { 
                                                                    var now = new Date();
                                                                    var end = new Date('2022-05-21T23:59+05:00');
                                                                    if (now < end)  $('.alerts-container').removeClass('empty');
                                                                   }
                                                                */

                                                                $('#form-list a').click(function (e) {
                                                                    $('#form-list a').attr('aria-expanded', 'false');
                                                                    $('#form-list a').removeClass('-expanded');
                                                                    $('form').hide();
                                                                    $('.personal-business,.commercial').hide();
                                                                    $(this).attr('aria-expanded', 'true');
                                                                    $(this).addClass('-expanded');
                                                                    $($(this).attr('href')).show();
                                                                    $($(this).attr('href').replace('#', '.').replace('-form', '') + ':not(.closed)').show();
                                                                    e.preventDefault();
                                                                });

                                                                $('.login-form-v2 input[type="text"], .login-form-v2 input[type="password"]').on('focus', function () {
                                                                    var id = $(this).attr('id'); $('label[for="' + id + '"]').addClass('-to-top');
                                                                });

                                                                $('.login-form-v2 input[type="text"], .login-form-v2 input[type="password"]').on('change input animationstart', function () {
                                                                    if ($(this).val() != '') {
                                                                        var id = $(this).attr('id');
                                                                        $('label[for="' + id + '"]').addClass('-to-top');
                                                                    }
                                                                });

                                                                $('.login-form-v2 input[type="text"], .login-form-v2 input[type="password"]').keypress(function (e) {
                                                                    var keycode = (e.keyCode ? e.keyCode : e.which);
                                                                    if (keycode == '13') {
                                                                        $(e.currentTarget).closest('form').find('input[type="submit"]').eq(0).click();
                                                                    }
                                                                    // e.stopPropagation();
                                                                });

                                                                populateUserId();

                                                                if (document.location.hash.toLowerCase().indexOf('#commercial') != -1) {
                                                                    $('a[href="#commercial-form"]').eq(0).click();
                                                                }
                                                                if (typeof (document.referrer) !== 'undefined' && document.referrer != '') {
                                                                    try {
                                                                        var r = new URL(document.referrer.toLowerCase());
                                                                        if (r.hostname.indexOf('mtb.com') > -1 && r.pathname.indexOf('/commercial') == 0) $('a[href="#commercial-form"]').eq(0).click();
                                                                    }
                                                                    catch (err) { }
                                                                }

                                                                var uuid;
                                                                if (typeof (cdApi.uuid !== 'undefined')) uuid = cdApi.uuid;
                                                                $('#txtSessionId').val(uuid);

                                                                $('body').on('submit', '.login-form-v2', function (e) {
                                                                    var $this = $(this);

                                                                    if ($this.hasClass('personal-business')) {

                                                                        axp.deleteCookie('OLBRememberUserId');
                                                                        axp.deleteCookie('PersonalUserID');
                                                                        if ($('#personal-login-checkbox').is(':checked')) {
                                                                            axp.setCookie({
                                                                                name: 'PersonalUserID',
                                                                                value: $('#txtUserID').val(),
                                                                                expires: 999
                                                                            });
                                                                        }

                                                                        try {
                                                                            utag.link({
                                                                                tealium_event: 'clickAction',
                                                                                customLinkName: 'userid:' + $('#txtUserID').val()
                                                                            });
                                                                        }
                                                                        catch (e) { }

                                                                        if (typeof (cdApi) !== 'undefined') {
                                                                            try { cdApi.client.flush(); } catch (e) { console.log('...error flushing cdApi: ' + e); }
                                                                        }

                                                                    }
                                                                    else if ($this.hasClass('commercial')) {

                                                                        axp.deleteCookie('user-id');
                                                                        axp.deleteCookie('TreasuryCenterUserID');
                                                                        if ($('#commercial-login-checkbox').is(':checked')) {
                                                                            axp.setCookie({
                                                                                name: 'TreasuryCenterUserID',
                                                                                value: $('#loginId').val(),
                                                                                expires: 999
                                                                            });
                                                                        }

                                                                        try {
                                                                            utag.link({
                                                                                tealium_event: 'clickAction',
                                                                                customLinkName: 'tcuserid:' + $('#loginId').val()
                                                                            });
                                                                        }
                                                                        catch (e) { }

                                                                    }

                                                                });

                                                                $('a.accordion').click(function (e) {
                                                                    if ($(this).hasClass('-expanded')) {
                                                                        $(this).removeClass('-expanded');
                                                                        $(this).attr('aria-expanded', 'false');
                                                                        $($(this).attr('href')).hide();

                                                                    }
                                                                    else {
                                                                        $(this).addClass('-expanded');
                                                                        $(this).attr('aria-expanded', 'true');
                                                                        $($(this).attr('href')).show();
                                                                    }
                                                                    e.preventDefault();
                                                                });

                                                                $('.cta-button-link[href="#login-modal"]').hide();

                                                                $('.alert-item button.close').click(function () {
                                                                    $(this).parent().hide();
                                                                    $(this).parent().addClass('closed');
                                                                });

                                                            });

                                                            function populateUserId() {
                                                                var id; var p = false;
                                                                id = Utils.cookies.getCookie('OLBRememberUserId');
                                                                if (typeof (id) === 'undefined' || id == '') id = Utils.cookies.getCookie('PersonalUserID');
                                                                if (typeof (id) !== 'undefined' && id != '') {
                                                                    p = true;
                                                                    $('#txtUserID').val(id);
                                                                    $('label[for="txtUserID"]').addClass('-to-top');
                                                                    $('#personal-login-checkbox').attr('checked', true);
                                                                }
                                                                id = Utils.cookies.getCookie('user-id');
                                                                if (typeof (id) === 'undefined' || id == '') id = Utils.cookies.getCookie('TreasuryCenterUserID');
                                                                if (typeof (id) !== 'undefined' && id != '') {
                                                                    $('#loginId').val(id);
                                                                    $('label[for="loginId"]').addClass('-to-top');
                                                                    $('#commercial-login-checkbox').attr('checked', true);
                                                                    if (p == false) $('a[href="#commercial-form"]').eq(0).click();
                                                                }
                                                            }

                                                            function cdSession() {

                                                                var uuid = Utils.cookies.getCookie('cdSessionId');
                                                                if (typeof (uuid) === 'undefined') {
                                                                    uuid = setCookie();
                                                                }
                                                                else {
                                                                    uuid = decodeURIComponent(uuid);
                                                                    var pattern = /^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$/i;
                                                                    if (!uuid.match(pattern)) uuid = setCookie();
                                                                }
                                                                if (typeof (cdApi) !== 'undefined') {
                                                                    cdApi.setCustomerSessionId(uuid);
                                                                    cdApi.uuid = uuid;
                                                                    cdApi.changeContext(getPageName());
                                                                }

                                                                function uuidv4() {
                                                                    var cr = window.crypto || window.msCrypto;
                                                                    return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                                                                        return (c ^ cr.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
                                                                    });
                                                                }

                                                                function setCookie() {
                                                                    uuid = uuidv4();
                                                                    Utils.cookies.removeCookie('cdSessionId');
                                                                    var dt = new Date();
                                                                    dt.setTime(dt.getTime() + 15 * 60 * 1000);
                                                                    var xp = ';expires=' + dt.toUTCString() + ';';
                                                                    document.cookie = 'cdSessionId=' + encodeURIComponent(uuid) + xp;
                                                                    return uuid;
                                                                }

                                                            }

                                                            cdSession();

                                                        </script>
